char const* sourceCode = R"(
	contract c {
		uint80[] x;
		function test() returns (uint80, uint80, uint80, uint80) {
			x.push(1);
			x.push(2);
			x.push(3);
			x.push(4);
			x.push(5);
			x.length = 4;
			return (x[0], x[1], x[2], x[3]);
		}
	}
)";
compileAndRun(sourceCode);
ABI_CHECK(callContractFunction("test()"), encodeArgs(1, 2, 3, 4));